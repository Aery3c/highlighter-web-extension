{
  "manifest_version": 3,
  "name": "highlighter",
  "description": "Base Level Extension",
  "version": "1.0.0",
  "action": {},
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "./background/background.js",
    "type": "module"
  },
  "content_scripts": [{
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": [
      "./contentScript/popperUI/index.js"
    ]
  }]
}